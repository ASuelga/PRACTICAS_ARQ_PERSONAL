            ORG 1000H
            CHAR DB "1H"
            
            ORG 2000H
            MOV BX, OFFSET CHAR  ;PRINCIPIO DE LA CADENA
            MOV AL, 1               ;CANTIDAD QUE IMPRIMO
    REPETIR:INT 7                   ;IMPRIMO EN PANTALLA
            INC BYTE PTR [BX]       ;PASO A OTRO ASCII
            CMP BYTE PTR [BX], 7FH  ;COMPARO SI NO ES 0
            JNZ REPETIR
            INT 0    ;hlt
            END
