             ORG 1000H
        NUM1 DW  6
        NUM2 DW  3
        RES  DW  ?
            
            ORG 3000H
SUBRRUTINA:
        MOV DX,0      ;RESULTADO A 0
        MOV AX, NUM1  ;NUM1 AX
        CMP AX,0      ;MIENTRAS NO SEA 0
        JZ FIN        ;SALTO A FIN
        MOV BX, NUM2  ;NUM2 BH
        CMP BX,0      ;MIENTRAS NO SEA 0
        JZ FIN        ;SALTO A FIN
   MULT:ADD DX,AX      ;SUMO 1 VEZ EN DX
        DEC BX         ;RESTO UNA VEZ
        JNZ MULT       ;SI ES 0 FIN
    FIN:RET

        ORG 2000H
        CALL SUBRRUTINA
    FIN:MOV RES, DX
        HLT
        END
